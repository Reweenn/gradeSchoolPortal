generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Student {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  grade     String?
  status    String?  @default("active")
  createdAt DateTime @default(now())
}
